<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="AppWS"
                   namespace="AppWS.Dominio"
                   default-lazy="false">
  <class name="Pedido_det" table="t_pedidodet">
    <id name="Codigo" column="codigo">
      <generator class="increment"/>
    </id>
    <property name="NPedido" column="npedido" />
    <property name="CodigoArticulo" column="codigoarticulo" />
    <property name="Articulo" column="articulo" />
    <property name="Cantidad" column="cantidad" />
  </class>
</hibernate-mapping>

