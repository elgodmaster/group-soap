<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="AppWS"
                   namespace="AppWS.Dominio"
                   default-lazy="false">
    <class name="Proveedor" table="t_proveedor">
        <id name="Codigo" column="codigo">
            <generator class="increment"/>
        </id>
        <property name="Nombre" column="nombre" />
        <property name="Ruc" column="ruc" />
        <property name="Direccion" column="direccion" />
        <property name="Telefono" column="telefono" />
        <property name="Fax" column="fax" />
        <property name="NombreContacto" column="nombreContacto" />
        <property name="TelefonoContacto" column="telefonoContacto" />
    </class>
</hibernate-mapping>